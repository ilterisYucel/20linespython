    "def get_word_count(in_file):",
    "word_count_dict = {}",
    "read_file = open(in_file, 'r')",
    "for line in read_file.readlines():",
    "line = line.strip(\"\\n\")",
    "for word in line.split(\" \"):",
    "if(word not in word_count_dict.keys()):",
    "word_count_dict[word] = 1",
    "else:",
    "word_count_dict[word] += 1",
    "read_file.close()",
    "return word_count_dict",
    "def print_word_count(in_file, out_file):",
    "write_file = open(out_file, 'w')",
    "for k,v in get_word_count(in_file).items():",
    "print(\"{} ---> {}\".format(k, v), file = write_file)",
    "write_file.close()",
